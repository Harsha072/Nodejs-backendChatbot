{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@now/node"
        }
    ]
    ,
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/api/requestText",
            "methods": [
                "POST"
            ],
            "dest": "index.js"
        },
        {
            "src": "/api/quiz",
            "methods": [
                "GET"
            ],
            "dest": "index.js"
        }
    ]
}